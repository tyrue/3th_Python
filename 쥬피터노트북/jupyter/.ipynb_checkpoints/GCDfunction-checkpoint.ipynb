    "def gcd(n1, n2):\n",
    "    gcd = 1\n",
    "    k = 2\n",
    "    \n",
    "    while k <= n1 and k <= n2:\n",
    "        if n1 % k == 0 and n2 % k == 0:\n",
    "            gcd = k\n",
    "        k += 1\n",
    "    return gcd"
   ]
  }
